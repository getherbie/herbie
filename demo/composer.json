{
    "name": "getherbie/demo",
    "description": "Demo application for the Herbie CMS. Herbie is a file based CMS & blog system and not ready for production use yet."
    "type": "project",
    "minimum-stability": "dev",
    "license":"BSD-3-Clause",
    "require": {
        "php": ">=5.4.0",
        "getherbie/herbie": "dev-master",
        "getherbie/composer": "dev-master"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "herbie\\composer\\Installer::setPermission"
        ]
    },
    "extra": {
        "writable": [
            "site/cache/data",
            "site/cache/page",
            "site/cache/twig"
        ],
        "executable": []
    }
}
